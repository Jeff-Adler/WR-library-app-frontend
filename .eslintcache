0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/reportWebVitals.js",[],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/App.js",[],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/index.js",[],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/views/Home.js",[],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/views/BookViews/Book.js",["55"],"import React, {useState} from 'react';\nimport { Link, useParams, useHistory, useRouteMatch  } from 'react-router-dom';\nimport {ListGroup, ListGroupItem, Button } from 'reactstrap'\n\nexport default function Book(props) {\n    let history = useHistory();\n    let { path } = useRouteMatch();\n    const [displayReferences, setDisplayReferences] = useState(false)\n    const [selectedReference, setSelectedReference] = useState()\n    const [isLoading, setIsLoading] = useState(false)\n\n    let { bookId } = useParams();\n    const {books, convertBookToAlt} = props  \n    // Finds the book to be shown\n    const book = books.find(book => book.id === parseInt(bookId))\n\n    \n    const mapAuthors = () => {\n        return book.authors.map((author) => {\n            return (\n                <ListGroupItem key={`author_${author.id}`} tag={Link} onClick={() => history.push(`/authors/${author.id}`)}>\n                    {`${author.first_name} ${author.last_name}`}\n                </ListGroupItem>\n            )\n        })\n    }\n    \n    const mapAlts = () => {\n        return book.alts.map((alt) => {\n            return (\n                <ListGroupItem key={`alt_${alt.id}`} tag={Link} to={`${bookId}/alts/${alt.id}`}>\n                    {alt.title}\n                </ListGroupItem>\n            )\n        })\n    }\n    \n    const mapReferences = () => {        \n        return books.map((book) => {\n            // removes self.book from dropdown\n            if (book.id === parseInt(bookId)) {\n                return \"\"\n            } else {\n                return <option key={`book_${book.id}`} value={`${book.id}`}>{book.title}</option>\n            }\n        })\n    }\n    \n    const bookConvertHandler = (e) => {\n        e.preventDefault()\n        if (selectedReference) {\n            setIsLoading(!isLoading)\n            convertBookToAlt(bookId, selectedReference)\n        } else {\n            alert(\"Please select a reference.\")\n        }\n    }\n\n    return (\n        <>\n            { isLoading ? \n                <div className=\"loading-screen\">\n                    <h3>Loading</h3>\n                </div>\n                :\n                <div className=\"list-container\">\n                    <ListGroup>\n                        <ListGroupItem className=\"center\">\n                            <strong>{book.title}</strong><br/>\n                        </ListGroupItem>\n                        <ListGroupItem>\n                            <strong>Author(s): </strong><br/>\n                        </ListGroupItem>\n                        {mapAuthors()}\n                        <ListGroupItem>\n                            <strong>Duplicate(s): </strong><br/>\n                        </ListGroupItem>\n                        {mapAlts()}\n                    </ListGroup><br/><br/>\n                    <div className=\"center\">\n                        <Button onClick={() => setDisplayReferences(!displayReferences)}>\n                            Make duplicate?\n                        </Button><br/><br/>\n                    </div>\n                    {displayReferences\n                    ? \n                    <div className=\"center\">\n                        <select name=\"cars\" id=\"cars\" onChange={e => {setSelectedReference(e.target.value)}}>\n                            {mapReferences()}\n                        </select><br/><br/>\n                        <Button onClick={bookConvertHandler}>\n                            Submit\n                        </Button>\n                    </div>\n                    : \n                    \"\"\n                    }\n                </div>\n            }\n        </>\n    )\n}","/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/containers/BookContainer.js",[],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/views/BookViews/BookList.js",[],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/containers/AltContainer.js",[],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/views/BookViews/Alt.js",[],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/containers/AuthorContainer.js",[],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/views/AuthorViews/Author.js",["56"],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/views/AuthorViews/AuthorList.js",[],"/Users/jeffadler/Development/code/take_home_assignments/WR-library-app/wr-library-app-frontend/src/views/NavBar.js",[],{"ruleId":"57","severity":1,"message":"58","line":7,"column":11,"nodeType":"59","messageId":"60","endLine":7,"endColumn":15},{"ruleId":"57","severity":1,"message":"61","line":3,"column":35,"nodeType":"59","messageId":"60","endLine":3,"endColumn":41},"no-unused-vars","'path' is assigned a value but never used.","Identifier","unusedVar","'Button' is defined but never used."]